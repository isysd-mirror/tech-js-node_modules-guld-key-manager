{
  "expo": {
    "name": "guld-browser",
    "description": "Browse the guld blocktree",
    "slug": "guld-browser",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/favicon.png",
    "splash": {
      "image": "./src/assets/images/logo-notext-black.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
